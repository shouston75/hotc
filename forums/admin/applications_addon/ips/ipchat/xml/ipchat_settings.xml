      <conf_key>ipschat_group_access</conf_key>
      <conf_default/>
      <conf_extra>#show_groups#</conf_extra>
      <conf_evalphp><![CDATA[if ( $save == 1)
{
	if ( is_array($_POST['ipschat_group_access']) )
	{
		$_POST['ipschat_group_access'] = implode(",",$_POST['ipschat_group_access']);
